---
import { Project } from "../content/config";
import ProjectPreview from "./ProjectPreview.astro";

interface Props {
  projects: Project[];
}

const { projects } = Astro.props as Props;
---

<div class="grid items-start gap-12 md:grid-cols-3">
  {projects.map((project) => <ProjectPreview {project} />)}
</div>
